programa {
  funcao inicio() {
    //declaração de variaveis
    real celsius, fahrenheint
    //declaração de constantes
    const inteiro B=32
    const real C=9.0
    const real A=5.0
    //entrada de dados
    escreva("informe a temperatura em graus celsius:")
    leia(celsius)
    //processamento
    fahrenheint= celsius*(C/A)+B
    //Saida de dados
    escreva("a temperatura de ",celsius," graus celsius em fahrenheint é: ",fahrenheint,"F")
  }
}
