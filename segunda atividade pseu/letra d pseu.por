programa {
  funcao inicio() {
    Inicio
    // Declaração de variáveis
    real celsius, fahrenheint;

    // Declaração de constantes
    constante inteiro A <- 9;
    constante inteiro B <- 160;

    // Entrada de dados
    escreva("Temperatura em Celsius: ");
    leia(celsius);

    // Processamento
    fahrenheint <- (A * celsius + B) / 5;

    // Saída de dados
    escreva("A temperatura informada, em Fahrenheit é: ", fahrenheint);
Fim

  }
}
