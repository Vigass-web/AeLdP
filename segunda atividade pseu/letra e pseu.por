programa {
  funcao inicio() {
    Inicio
    // Declaração de variáveis
    real celsius, fahrenheint;

    // Declaração de constantes
    constante inteiro A <- 32;
    constante inteiro B <- 5;
    constante inteiro C <- 9;

    // Entrada de dados
    escreva("Temperatura em Fahrenheit: ");
    leia(fahrenheint);

    // Processamento
    celsius <- ((fahrenheint - A) * B) / C;

    // Saída de dados
    escreva("A temperatura informada, em Celsius é: ", celsius);
Fim


  }
}
