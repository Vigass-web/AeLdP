programa {
  funcao inicio() {
    //declaração de variáveis
  real celsius, fahrenheint
  //declaração de constantes
  const inteiro A=9
  const inteiro B=160
  //entrada de dados
  escreva("temperatura em celsius: ")
  leia(celsius)
  //processamento
  fahrenheint=(A*celsius+B)/5
  //saida de dados
  escreva("a temperatura informada, em fahrenheint é: ",fahrenheint)
  }
}
