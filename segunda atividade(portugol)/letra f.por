programa {
  funcao inicio() {
    //declaração de variáveis
  real volume, raio, altura
  //declaração de constantes
  const real A=3.141559
  //entrada de dados
  escreva("informe altura: ")
  leia(altura)
  escreva("informe raio: ")
  leia(raio)
  //processamento
  volume=A*raio*altura
  //saida de dados
  escreva("o volume interno é:",volume)
  }
}
