programa {
  funcao inicio() {
    //declaração de variáveis
  real celsius, fahrenheint
  //declaração de constantes
  const inteiro A=32
  const inteiro B=5
  const inteiro C= 9
  //entrada de dados
  escreva("temperatura em fahrenheint: ")
  leia(fahrenheint)
  //processamento
  celsius=((fahrenheint-A)*B)/9
  //saida de dados
  escreva("a temperatura informada, em celsius é: ",celsius)
  }
}
